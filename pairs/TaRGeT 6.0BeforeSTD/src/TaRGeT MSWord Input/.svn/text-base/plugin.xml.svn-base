<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="br.ufpe.cin.target.pm.input">
      <inputDocumentExtension
            inputDocumentErrorIcon="icons/word_icon_error_32_32.bmp"
            inputDocumentIcon="icons/word_icon_32_32.bmp"
            inputDocumentImplementation="br.ufpe.cin.target.mswordinput.controller.MSWordDocumentExtensionImplementation"
            inputDocumentType="doc"
            inputDocumentTypeFilter="*.doc"
            name="MS Word Documents">
      </inputDocumentExtension>
   </extension>
   <extension
         point="br.ufpe.cin.target.core.startingOperations">
      <startingOperationsExtension
            startingOperationsImplementation="br.ufpe.cin.target.mswordinput.controller.MSWordStartingOperationsExtensionImplementation">
      </startingOperationsExtension>
   </extension>

</plugin>
